

{% block body %}
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    </head>

    <script type="text/javascript">

        $(document).ready(function () {
            window.print();
        });

    </script>
    <h1>Evénements</h1>

    <table class="table">
        <tbody>

        <tr>
            <th>Id</th>
            <td>{{ evenement.id }}</td>
        </tr>
        <tr>
            <th>Nom de l'événement</th>
            <td>{{ evenement.nomevent }}</td>
        </tr>
        <tr>
            <th>Durée</th>
            <td>{{ evenement.duree }}</td>
        </tr>
        <tr>
            <th>Maxparticipants</th>
            <td>{{ evenement.maxParticipants }}</td>
        </tr>
        <tr>
            <th>Dated</th>
            <td>{% if evenement.dateD %}{{ evenement.dateD|date('Y-m-d') }}{% endif %}</td>
        </tr>
        <tr>
            <th>Datef</th>
            <td>{% if evenement.dateF %}{{ evenement.dateF|date('Y-m-d') }}{% endif %}</td>
        </tr>
        <tr>
            <th>Gagnant</th>
            <td>{{ evenement.gagnant }}</td>
        </tr>
        <tr>
            <th>Etat</th>
            <td>{{ evenement.etat }}</td>
        </tr>
        <tr>
            <th>Region</th>
            <td>{{ evenement.region }}</td>
        </tr>
        <tr>
            <th>Image</th>
            <td>{{ evenement.image }}</td>
        </tr>
        </tbody>
    </table>



{% endblock %}